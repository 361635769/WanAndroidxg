<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px80">

        <ImageView
            android:id="@+id/backtitle"
            android:layout_width="@dimen/px50"
            android:layout_height="@dimen/px50"
            android:layout_marginTop="@dimen/px15"
            android:layout_marginLeft="@dimen/px30"
            android:background="@drawable/icon_back"
            />

        <TextView
            android:id="@+id/titlecontent"
            android:layout_width="@dimen/px500"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/px100"
            android:textColor="#888888"
            android:gravity="center_vertical"
            android:maxLines="2"
            />
        
        
        <ImageView
            android:id="@+id/moretitle"
            android:layout_width="@dimen/px44"
            android:layout_height="@dimen/px44"
            android:layout_alignParentRight="true"
            android:background="@drawable/icon_more"
            android:layout_marginTop="@dimen/px18"
            android:layout_marginRight="@dimen/px18"
            />
    </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#dddddd"/>

    <LinearLayout
        android:id="@+id/mweblin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


    </LinearLayout>

</LinearLayout>
